<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.zopera.mapper.NoticeMapper">

    <resultMap id="BaseResultMap" type="com.example.zopera.entity.Notice">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="picUrl" column="pic_url" jdbcType="VARCHAR"/>
            <result property="picTitle" column="pic_title" jdbcType="VARCHAR"/>
            <result property="picContent" column="pic_content" jdbcType="VARCHAR"/>
            <result property="publisher" column="publisher" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,pic_url,pic_title,
        pic_content,publisher,create_time
    </sql>
</mapper>
